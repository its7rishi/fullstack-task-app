{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(,*)",
      "dest": "/api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "UPDATE", "OPTIONS"],
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
