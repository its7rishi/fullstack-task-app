{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(,*)",
      "dest": "/api/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "UPDATE", "OPTIONS"]
    },

    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/api/*",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
